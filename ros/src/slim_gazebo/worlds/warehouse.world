<?xml version="1.0" ?>
<sdf version="1.4">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <!-- The shelf -->
    <include>
      <uri>model://kiva_pod</uri>
    </include>
    <!-- table for the robot -->
    <include>
      <uri>model://table</uri>
      <pose>0 -1 0 0 0 0</pose>
    </include>
  </world>
</sdf>
